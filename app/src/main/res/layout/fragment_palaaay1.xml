        app:layout_constraintTop_toBottomOf="@id/img_game_elem_5" />


    <ImageView
        android:id="@+id/img_game_elem_12"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/vv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/img_game_elem_5"
