    <ImageView
        android:id="@+id/img_game_elem_5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/diiiimond"
        app:layout_constraintEnd_toStartOf="@id/img_game_elem_6"
